{
  "name": "mqttr",
  "version": "3.0.5",
  "description": "A routable mqtt library based on mqtt.js",
  "homepage": "https://github.com/taoyuan/mqttr",
  "author": {
    "name": "taoyuan",
    "email": "towyuan@outlook.com",
    "url": "https://github.com/taoyuan"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "mqtt",
    "router",
    "client"
  ],
  "repository": "https://github.com/taoyuan/mqttr",
  "scripts": {
    "prerelease": "cross-env CI=1 npm run test",
    "release": "np --no-2fa",
    "build": "nb-tsc -b",
    "build:full": "npm ci --ignore-scripts && npm run clean && npm run build",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "eslint": "nb-eslint --report-unused-disable-directives --cache .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier:cli": "nb-prettier \"**/*.ts\" \"**/*.js\" \"**/*.md\"",
    "prettier:check": "npm run prettier:cli -- --check",
    "prettier:fix": "npm run prettier:cli -- --write",
    "clean": "nb-clean dist coverage .nyc_output *.tsbuildinfo",
    "pretest": "npm run clean && npm run prettier:fix && npm run build",
    "test": "npm run jest",
    "test:ci": "jest --runInBand --ci --coverage",
    "jest": "jest --runInBand",
    "posttest": "npm run lint"
  },
  "dependencies": {
    "async-mqtt": "^2.6.1",
    "bl": "^5.0.0",
    "debug": "^4.3.3",
    "msgpack5": "^5.3.2",
    "path-to-regexp": "^6.2.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@jil/common": "^0.2.9",
    "@nutol/build": "^0.2.1",
    "@nutol/eslint-config": "^0.2.0",
    "@types/debug": "^4.1.7",
    "@types/jest": "^27.4.0",
    "@types/micromatch": "^4.0.2",
    "@types/msgpack5": "^3.4.2",
    "@types/node": "^17.0.10",
    "@types/uniqid": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "aedes": "^0.46.2",
    "cross-env": "^7.0.3",
    "docker-cli-js": "^2.9.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-eslint-plugin": "^4.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "get-port": "^5.1.1",
    "jest": "^27.4.7",
    "jest-junit": "^13.0.0",
    "np": "^7.6.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4"
  },
  "optionalDependencies": {
    "ws": "^8.4.2"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.com/",
    "access": "public"
  }
}
